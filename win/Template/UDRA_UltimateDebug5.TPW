#! %ProcedureTemplate = 'Source'
#!---------------------------------------------------------------------
#AT(%ProcessedCode),PRIORITY(0005)
 #IF(%TraceCode AND ~%CLSkelAppDisable)
! === %ProcedureTemplate ===
  #IF(%TimingCode)
  rStarDate = oDateTime.GetStarDate() #<! Set up procedure entry date+time
  #ENDIF
  %CLSkelClass.DebugEntry('%Application (%Module): %Procedure (%ProcedureTemplate)') #<! Ultimate Debug ENTRY %ProcedureTemplate
 #ENDIF
 #IF(%TraceCode AND ~%CLSkelAppDisable AND %ShowLocalTemplates)
  #IF(ITEMS(%ActiveTemplate))
  %CLSkelClass.DebugLevel('----------------> LOCAL TEMPLATES')
   #FOR(%ActiveTemplate)
  %CLSkelClass.DebugLevel('Active Templates: %[18]ActiveTemplateType %ActiveTemplate ')
   #ENDFOR
  %CLSkelClass.DebugLevel('---------------->')
  #ENDIF
! === %ProcedureTemplate ===
 #ENDIF
#ENDAT
#!---------------------------------------------------------------------
#AT(%ProcessedCode),PRIORITY(9995)
 #IF(%TraceCode AND ~%CLSkelAppDisable)
! === %ProcedureTemplate ===
  #IF(%TimingCode)
  %CLSkelClass.DebugExit('%Application (%Module): %Procedure (%ProcedureTemplate) [' & oDateTime.FormatOneDayTime(oDateTime.CalcStardateDelta(rStarDate)) & ']') #<! Ultimate Debug EXITS %ProcedureTemplate
  %QueueName.Find('%Application-%Procedure',oDateTime.CalcStardateDelta(rStarDate))
  #ELSE
  %CLSkelClass.DebugExit('%Application (%Module): %Procedure (%ProcedureTemplate)') #<! Ultimate Debug EXITS %ProcedureTemplate
  #ENDIF
! === %ProcedureTemplate ===
 #ENDIF
#ENDAT
