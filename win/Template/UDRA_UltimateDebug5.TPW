#AT(%WindowManagerMethodCodeSection,'Init','(),BYTE'),PRIORITY(5090)
 #IF(%TraceCode AND ~%CLSkelAppDisable)
  #COMMENT(90)
! === %ProcedureTemplate ===
IF ReturnValue THEN
  #IF(%TimingCode)
  %CLSkelClass.DebugExit('%Application (%Module): %Procedure (%ProcedureTemplate) INIT Short-Stop [' & oDateTime.FormatOneDayTime(oDateTime.CalcStardateDelta(rStarDate)) & ']') #<! Ultimate Debug EXITS %ProcedureTemplate
  %QueueName.Find('%Application-%Procedure',oDateTime.CalcStardateDelta(rStarDate))
  #ELSE
  %CLSkelClass.DebugExit('%Application (%Module): %Procedure (%ProcedureTemplate) INIT Short-Stop') #<! Ultimate Debug EXITS %ProcedureTemplate
  #ENDIF
  RETURN ReturnValue
END
! === %ProcedureTemplate ===
  #COMMENT(60)
 #ENDIF
#ENDAT
